.car-modal-list
  position absolute
  bottom 0px
  width 100%
  padding 10px
  margin-bottom 40px
  .cart-item
    position relative
    padding 15px
    height 50px
    .info
      position absolute
      right 110px
      left 20px
      &>p
        float left
      &>p + p
        padding-left 5px
      .name
        overflow hidden
        text-overflow ellipsis
        
  .car-modal-list__add-cart
    position absolute
    right 0px
    top 10px
    