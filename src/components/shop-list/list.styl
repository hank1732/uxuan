.u-list
  a
    display block
  .img
    float left
    width 60px
    height 60px
    border-radius 100px
    text-align center
    img
      width 100%
      height 100%
      border-radius 100px
      
  .full-desc
    white-space normal
    
  .info-wrap
    margin 0 20px 0 75px
    font-size 12px
    color #808080
    .name
      display inline-block
      width 100%
      font-size 17px
      color #333
      overflow hidden
      text-overflow ellipsis
    .full-name
      font-size 17px
      color #333
      white-space normal
    .distance
      float right
    .send-divide
      padding 0 8px
      color #e6e6e6
    .quantity
      line-height 30px
      
  .top
    position absolute
    top 12px
    right 20px
    width 35px
    text-align center
    color #6e96c7
    .top-header
      border-radius 5px 5px 0 0
      background rgba(97, 157, 229, 0.4)
    .top-body
      background #d4efff

.divider
  text-align center
  font-size 18px
  background #fff
  overflow hidden
